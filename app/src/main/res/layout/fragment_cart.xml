<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gs.ibeacon.fcu.slideswipe.Fragment.CartFragment"
    android:orientation="vertical">

    <!-- TODO: Update blank fragment layout -->


    <info.hoang8f.widget.FButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="50dp"
        android:text="D"
        android:id="@+id/btnD"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


    <info.hoang8f.widget.FButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="50dp"
        android:text="L"
        android:id="@+id/btnL"
        android:layout_above="@+id/btnD"
        android:layout_toStartOf="@+id/btnD" />

    <info.hoang8f.widget.FButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="50dp"
        android:text="R"
        android:id="@+id/btnR"
        android:layout_above="@+id/btnD"
        android:layout_toEndOf="@+id/btnD"  />


    <info.hoang8f.widget.FButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="50dp"
        android:text="S"
        android:id="@+id/btnS"
        android:layout_above="@+id/btnD"
        android:layout_centerHorizontal="true"  />


    <info.hoang8f.widget.FButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:text="U"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="50dp"
        android:id="@+id/btnU"
        android:layout_above="@+id/btnL"
        android:layout_centerHorizontal="true"  />


    <info.hoang8f.widget.FButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="導引"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="20dp"
        android:id="@+id/guideToTargetButton"
        android:layout_above="@+id/btnD"
        android:layout_alignParentStart="true" />

    <info.hoang8f.widget.FButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="移動"
        fbutton:buttonColor="@color/buttonColor"
        fbutton:shadowColor="@color/black_de"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        fbutton:cornerRadius="20dp"
        android:id="@+id/moveToTargetButton"
        android:layout_above="@+id/btnU"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text=""
        android:id="@+id/cartLocationText"
        android:layout_gravity="center_horizontal|top"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp" />

</RelativeLayout>



